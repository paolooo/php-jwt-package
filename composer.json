{
    "name": "paolooo/php-jwt-package",
    "description": "PHP JWT Implementation.",
    "keywords": [
        "league",
        "paolooo",
        "php-jwt-package",
        "php-jwt",
        "jwt"
    ],
    "homepage": "https://github.com/paolooo/php-jwt-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Nino Paolo Amarillento",
            "email": "pol@ninopaolo.com",
            "homepage": "http://ninopaolo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "emarref/jwt": "^0.1.1",
        "cartalyst/sentinel": "^2.0",
        "illuminate/auth": "^5.1",
        "illuminate/contracts": "^5.1",
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Paolooo\\Schnauzer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paolooo\\Schnauzer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
